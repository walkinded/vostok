mixin paginator()
  .pager.clear
    ul.page-numbers
      li
        a.prev.page-numbers(href='#')

      li.current
        a.page-numbers(href='#') 1
      li
        span.page-numbers.current(aria-current="page") 2
      li
        a.page-numbers(href='#') 3

      li
        a.next.page-numbers(href='#')


  // /.paginator
